        android:supportsRtl="true"
        android:theme="@style/Theme.Car"
        tools:targetApi="31">
        <activity
            android:name=".MainPage"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <!-- MyPage 액티비티를 추가합니다 -->
        <activity android:name=".LoginPage" android:exported="true" />
        <activity android:name=".ActivitySingup" android:exported="true" />
        <activity android:name=".MyPage" android:exported="true" />
        <activity android:name=".CarImgList" android:exported="true" />
        <activity android:name=".CarSelectList" android:exported="true" />
        <activity android:name=".Bottombar" android:exported="true" />
        <activity android:name=".AlarmList" android:exported="true" />
        <activity android:name=".AlarmCheck"></activity>

    </application>

    <meta-data
        android:name="com.kakao.sdk.AppKey"
        android:value="3fbcda37df6c8200964f637b0ec3a28b" />

    <!-- 인터넷 권한 필요 -->
    <uses-permission android:name="android.permission.INTERNET" />

    </manifest>